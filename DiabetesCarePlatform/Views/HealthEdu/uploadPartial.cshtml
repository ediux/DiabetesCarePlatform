@using DiabetesCarePlatform
@using DiabetesCarePlatform.Models
@model IEnumerable<ImagesViewModel>

    @{
        ViewBag.Title = "uploadPartial";
    }



    @foreach (var image in Model)
    {
        @*<a href="#" class="returnImage" data-url="@Request.Url.GetLeftPart(UriPartial.Authority)@image.Url">*@
           <a href="#" class="returnImage" data-url="@Request.Url.GetLeftPart(UriPartial.Authority)@image.Url">
            <img src="@image.Url" alt="Hejsan" id="#image" data-source="@image.Url" width="300" height="300" />
        </a>
    }
    <script type="text/javascript">
        $(document).ready(function () {
            $(".returnImage").click("click", function (e) {
                var urlImage = $(this).attr("data-url");
                window.opener.updateValue("cke_72_textInput", urlImage);
                window.close();
            });
        });
    </script>
