@model IEnumerable<DiabetesCarePlatform.Models.ZoomNetMeeting.ServiceTimeViewModel>
@foreach (var item in Model)
{
    <div class="form-group">
        <div class="col-md-12">
            <div class="checkbox">
                <label>
                    @if (Model.Where(w => w.TimeSectionId == item.TimeSectionId).Any())
                    {
                        <input type="checkbox" value="@item.TimeSectionId" checked />
                    }
                    else
                    {
                        <input type="checkbox" value="@item.TimeSectionId" />
                    }
                    @item.StartTime.ToString() ~ @item.EndTime.ToString()
                </label>
            </div>
        </div>
    </div>
}
